<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    tools:context=".MemberAddressUpdateActivity" >
  
    <ScrollView
        android:id="@+id/Address_form"
        android:background="@drawable/eligibility_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
		
		<LinearLayout
            style="@style/AddressFormContainer"
            android:orientation="vertical" >
         
         
         <FrameLayout
            android:id="@+id/fragment_content"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="5.13"
            android:animateLayoutChanges="true"
            android:background="@drawable/eligibility_layout"
            android:padding="1dp"
            android:scrollbars="vertical" >
		    
		    <LinearLayout
            	style="@style/AddressFormContainer"
            	android:orientation="vertical" >
			
			<TextView
	        	android:id="@+id/AddMemberName"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingBottom="10dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:textStyle="bold" />

		    <TextView
	        	android:id="@+id/lblAddress"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingBottom="3dp"
	        	android:paddingLeft="6dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:text="Address:"
	        	android:textStyle="normal" />
		    
		    <EditText
                android:id="@+id/Address1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
		    <TableRow
	    		android:layout_width="match_parent"
	        	android:layout_height="5dp"/>

		    <EditText
                android:id="@+id/Address2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
			
			<TableRow
	    		android:layout_width="match_parent"
	        	android:layout_height="5dp"/>
			<TextView
	        	android:id="@+id/lblCity"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingBottom="3dp"
	        	android:paddingLeft="6dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:text="City:"
	        	android:textStyle="normal" />
		    <EditText
                android:id="@+id/City"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
            <TableRow
	    		android:layout_width="match_parent"
	        	android:layout_height="5dp"/>
			<TextView
	        	android:id="@+id/lblState"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingBottom="3dp"
	        	android:paddingLeft="7dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:text="State:"
	        	android:textStyle="normal" />
            <!-- <Spinner
                    android:id="@+id/StateList"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >
                </Spinner>-->
            <EditText
                android:id="@+id/StateEdit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
             <TableRow
	    		android:layout_width="match_parent"
	        	android:layout_height="5dp"/>
		    <TextView
	        	android:id="@+id/lblZip"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingBottom="3dp"
	        	android:paddingLeft="7dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:text="Zip:"
	        	android:textStyle="normal" />
		    <EditText
                android:id="@+id/Zip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
		     <TableRow
	    		android:layout_width="match_parent"
	        	android:layout_height="5dp"/>
		    <TextView
	        	android:id="@+id/lblContact"
	        	android:layout_width="match_parent"
	        	android:layout_height="wrap_content"
	        	android:paddingLeft="7dp"
	        	android:paddingBottom="3dp"
	        	android:textAppearance="@style/LabelText"
	        	android:layout_weight="1"
	        	android:text="Phone:"
	        	android:textStyle="normal" />
		    <EditText
                android:id="@+id/Contact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="8sp"
                android:inputType="text"
                android:background="@drawable/editbox_border"
                android:maxLines="1"
                android:singleLine="true" />
 			</LinearLayout>
 			</FrameLayout>

		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:layout_marginTop="5dp"
		        android:orientation="horizontal" >

		        <Button
		            android:id="@+id/btnUpdate"
		            android:layout_width="0dp"
		            android:layout_height="40dp"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:background="@drawable/red_button"
		            android:text="Update"
		            android:textColor="#213C78" />

		    </LinearLayout>

        </LinearLayout>
    
    </ScrollView>
</merge>